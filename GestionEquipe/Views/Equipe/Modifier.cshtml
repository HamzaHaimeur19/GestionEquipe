@model GestionEquipe.Models.Equipe


<h1>Modifier</h1>

<h4>Equipe</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Equipe" asp-action="Modifier" method="post">
            <div class="form-group">
                <label asp-for="Pays" class="control-label"></label>
                <input asp-for="Pays" class="form-control" />
                <span asp-validation-for="Pays" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="nbrParticipation" class="control-label"></label>
                <input asp-for="nbrParticipation" class="form-control" />
                <span asp-validation-for="nbrParticipation" class="text-danger"></span>
            </div>
            <button class="btn btn-primary" type="submit">Modifier</button>
        </form>
    </div>
</div>

